# KenAI 环境变量配置示例
# 复制此文件为 .env.local 并根据实际情况修改配置

# ComfyUI 服务配置
COMFYUI_HOST=127.0.0.1
COMFYUI_PORT=8188
COMFYUI_PROTOCOL=http

# 应用配置
NEXT_PUBLIC_APP_NAME=KenAI
NEXT_PUBLIC_APP_VERSION=1.0.0

# 开发环境配置
NODE_ENV=development

# 生产环境配置
NODE_ENV=production

# ComfyUI 服务器地址
COMFYUI_URL=http://localhost:8188

# 可选：如果需要外部API密钥
# OPENAI_API_KEY=your_openai_api_key_here
# BAIDU_TRANSLATE_APP_ID=your_baidu_app_id
# BAIDU_TRANSLATE_SECRET=your_baidu_secret 
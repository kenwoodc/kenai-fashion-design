{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/api/comfyui/(.*)",
      "destination": "http://localhost:8188/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 